{
  "name": "Timeey",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "/*** ---> main scripts": "***/",
    "start": "node dist/src/main.js",
    "build": "tsc",
    "start:build": "npm run build && npm run start",
    "dev": "ts-node-dev src/main.ts",
    "/* copy the openapi docs to the dist folder": "*/",
    "/* -E: throws error if nothing is copied": "*/",
    "prebuild": "copyfiles -E ./src/docs/**/* ./dist",
    "/*** ---> test scripts": "***/",
    "test": "npm run build && mocha --slow 0 --file dist/tests/setupTests.js dist/tests/**/*.test.js",
    "test:dev": "mocha --slow 0 -r ts-node/register 'tests/**/*.test.ts' --file tests/setupTests.ts --watch --watch-files 'src/**/*,tests/**/*'",
    "test:coverage": "nyc npm run test",
    "/*** ---> lint and format scripts": "***/",
    "lint": "eslint --ext .js,.ts --max-warnings 0 src/ tests/",
    "lint:fix": "eslint --ext .js,.ts --max-warnings 0 --fix src/ tests/",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check .",
    "/*** --> husky and git hook scripts": "***/",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/logging-winston": "^5.3.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gaxios": "^5.1.3",
    "googleapis": "^121.0.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "redoc-express": "^2.1.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "yaml": "^2.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.2",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "chai": "^4.3.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "proxyquire": "^2.1.3",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
